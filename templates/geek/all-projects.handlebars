<div class="container-fluid contentALL">
    <div class="geekProfile row">
        <div class="menuBox col-sm-2 col-md-2 col-lg-2 col-xs-12">
            <nav class="navMenu">
                <ul>
                    <li><a href="/#/geek/profile">My profile</a></li>
                    <li><a href="/#/geek/watchlist">Watchlist</a></li>
                    <li><a href="/#/geek/projects">Projects</a></li>
                    <li><button class="button browse-job">Browse Job</button></li>
                </ul>
            </nav>
        </div>
        <div class="mainProfile col-sm-10 col-md-10 col-lg-10 col-xs-12">
            <span class="contentAbout">All projects you sent submissions to:</span>
            <div class="table-responsive">
                <table class="table table-striped table-hover ">
                    <thead>
                        <tr>
                            <th>Title</th>
                            <th>Project Owner</th>
                            <th>Due date</th>
                            <th>Price</th>
                            <th>Download</th>
                        </tr>
                    </thead>
                    <tbody id="append-body-projects">
                        
                    </tbody>
                </table> 
            </div>
        </div>
    </div>
</div>

<!-- In Progress -->
<div class="modal fade" id="progress" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <img class="img-responsive" src="../../images/logo.png" alt="logo"/>
      </div>
      <div class="modal-body">
        <p>The Submission is in progress</p>
      </div>
      <div class="modal-footer">
        
      </div>
    </div>
  </div>
</div>

<!-- Not Winner -->
<div class="modal fade" id="notWinner" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <img class="img-responsive" src="../../images/logo.png" alt="logo"/>

      </div>
      <div class="modal-body">
        <p>Sorry, but you are not a winner</p>
      </div>
      <div class="modal-footer">
        
      </div>
    </div>
  </div>
</div>

<!-- Winner -->
<div class="modal fade" id="winner" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <img class="img-responsive" src="../../images/logo.png" alt="logo"/>
            </div>
            <div class="modal-body">
                <h1>Congratulations you are the WINNER</h1>
                <form>
                    <div class="form-group">
                        <label for="card-holder">Cardholder's Name: </label>
                        <input type="text" class="form-control" id="card-holder" placeholder="Valentin Georgiev">
                    </div>
                    <div class="form-group">
                        <label for="card-number">Credit Card Number: </label>
                        <input type="text" class="form-control" id="card-number" placeholder="****************">
                    </div>
                    <div class="form-group">
                        <label for="security-code">CCV Code: </label>
                        <input type="text" class="form-control" id="security-code" placeholder="***">
                        <p class="help-block">These are the last 3 digits of the code printed on the reverse side of yout card</p>
                    </div>
                    <div class="form-group">
                        <label for="expiry-date">Expiry Date: </label>
                        <input class="form-control" type="month" id="expiry-date">
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button id="payment-info-btn" class="button">Send Payment Info</button>
            </div>
        </div>
    </div>
</div>















