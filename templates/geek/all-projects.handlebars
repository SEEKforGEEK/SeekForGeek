<!-- In Progress -->
<div class="modals modal fade" id="progress" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <header class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <img class="img-responsive" src="../../images/logo.png" alt="logo"/>
      </header>
      <section class="modal-body">
        <p class="inProgress">The Submission is in progress!</p>
      </header>
    </div>
  </div>
</div>

<!-- Not Winner -->
<div class="modals modal fade" id="notWinner" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <header class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <img class="img-responsive" src="../../images/logo.png" alt="logo"/>
      </header>
      <section class="modal-body">
        <p class="notWinner">Sorry, but you are not a winner.</p>
      </section>
    </div>
  </div>
</div>

<!-- Winner -->
<div class="modals modal fade" id="winner" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <header class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <img class="img-responsive" src="../../images/logo.png" alt="logo"/>
            </header>
            <section class="modal-body">
                <h1 class="winner">Congratulations!You are the WINNER!</h1>
                <span class="winnerInfo">/please include the following information/</span>
                <form>
                    <div id="payment-cardholer-form" class="form-group">
                        <label for="card-holder">Cardholder's Name: </label>
                        <input type="text" class="form-control" id="card-holder" placeholder="Valentin Georgiev">
                    </div>
                    <div id="payment-IBAN-form" class="form-group">
                        <label for="IBAN">IBAN: </label>
                        <input type="text" class="form-control" id="IBAN" placeholder="****************">
                    </div>
                </form>
            </section>
            <footer class="modal-footer">
                <button id="payment-info-btn" class="button">Send Payment Info</button>
            </footer>
        </div>
    </div>
</div>















